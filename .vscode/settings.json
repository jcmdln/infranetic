{
  "editor.wordWrapColumn": 99,
  "files.associations": {
    "${workspaceFolder}/.ansible-lint": "yaml"
  },
  "files.exclude": {
    ".cache": true,
    ".config": true,
    ".local": true,
    ".vagrant.d": true,
    "build": true,
  },
  "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
  "yaml.schemas": {
    "https://json.schemastore.org/github-workflow.json": "${workspaceFolder}/.github/workflows"
  }
}
